{
  "name": "@fatrex/nestjs-agenda",
  "version": "0.0.2",
  "description": "agenda module for Nestjs",
  "main": "dist/index.js",
  "files": [
    "dist",
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "fix": "rm -rf node_modules && rm package-lock.json && npm install",
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "format": "prettier --write \"**/*.ts\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/fatrex/nestjs-agenda.git"
  },
  "keywords": [
    "nestjs",
    "agenda"
  ],
  "contributors": [
    "Alex Chen",
    "Daniele Lenares"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fatrex/nestjs-agenda/issues"
  },
  "homepage": "https://github.com/fatrex/nestjs-agenda#readme",
  "peerDependencies": {
    "@nestjs/common": "^9.4.0",
    "@nestjs/core": "^9.4.0",
    "@types/agenda": "^4.1.0",
    "agenda": "^5.0.0"
  },
  "devDependencies": {
    "prettier": "2.8.8",
    "reflect-metadata": "^0.1.13",
    "release-it": "^15.10.3",
    "typescript": "^5.0.4"
  }
}
